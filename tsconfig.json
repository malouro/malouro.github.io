{
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx"
	],
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"types": ["cypress"],
		"typeRoots": [
			"node_modules/@types",
			"@material-ui/types",
			"src/types"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"strictNullChecks": false,
		"strictFunctionTypes": false,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react"
	}
}
